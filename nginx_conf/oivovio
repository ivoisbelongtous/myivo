server {
	listen 80 default_server;
	listen [::]:80 default_server;

	root /var/www/html;

	index index.html;

	server_name oivov.io;

	location / {
		try_files $uri $uri/ =404;
	}

	location /thedevzone/ {
		auth_basic "Dev area, stay out!";
		auth_basic_user_file conf/htpasswd;
	}
}

server {
    server_name www.oivov.io;
    
    return 301 $scheme://oivov.io$request_uri;
}